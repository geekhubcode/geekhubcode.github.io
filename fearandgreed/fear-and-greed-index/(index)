	<script>
Vue.component('fng-chart', {
	extends: LineChart,
	props: ['days'],
	data() {
		return {
			data: null,
			options: {
				maintainAspectRatio:false,
				responsive: true,
	            tooltips: {
	                mode: 'index',
	                intersect: false,
	            },
	            hover: {
	                mode: 'nearest',
	                intersect: true
	            },
	            scales: {
	                xAxes: [{
	                    display: true,
	                    scaleLabel: {
	                        display: false,
	                        labelString: 'Day'
	                    }
	                }],
	                yAxes: [{
	                    display: true,
	                    scaleLabel: {
	                        display: true,
	                        labelString: 'Value'
	                    },
	                    ticks: {
	                        beginAtZero: true,
	                        steps: 10,
	                        stepValue: 5,
	                        max: 100
	                    }
	                }]
	            }
	        }
		}
	},
	mounted() {
		this.getFearAndGreedHistory()
	},
	watch: {
		days() {
			this.getFearAndGreedHistory()
		}
	},
	methods: {
		async getFearAndGreedHistory() {
			try {
    			const { data } = await this.axios.post('/crypto/fear-and-greed-index/history', {
    				days: this.days
    			})

    			if (data.success) {
					this.renderChart(data.data, this.options)
				}
    		} catch(e) {
    			console.log(e)
    		}
		}
	}
});
</script>


<script>
new Vue({
	el: '#main',
	data() {
		return {
			fngHistoryDays: 30,
			widgetData: '',
			showSteps: false,
		}
	},
	mounted() {
		const promise = this.getWidgetFile()
	},
	methods: {
		toastCopied() {
			this.$buefy.toast.open({
				duration: 2000,
				message: 'Copied to clipboard',
				type: 'is-success'
			})
		},

		async getWidgetFile() {
			
			this.axios.get('/widget/fng_ios_widget.js', { baseURL: ''})
				.then((response) => {
					// console.log(response)
					this.widgetData = response.data
				});

		},
	}
});
</script>

